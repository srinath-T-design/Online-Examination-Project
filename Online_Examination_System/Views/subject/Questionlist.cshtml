@model IEnumerable<Bussiness_Object_Layer.Model.Question>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Questionlist</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Question_name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.subjectID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.optionID)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Question_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.subjectID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.optionID)
            </td>
        
        </tr>
    }
    
    </table>
</body>
</html>
